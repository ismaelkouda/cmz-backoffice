{
  "name": "cateli-data-collector",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "env:generate": "node tools/env/env-cli.js",
    "env:validate": "node scripts/env-cli.js --validate",
    "env:types": "node scripts/env-cli.js --types",
    "env:dev": "node tools/env/generate-env.js dev",
    "env:cloud": "node tools/env/generate-env.js cloud",
    "env:cmz_dev": "node tools/env/generate-env.js cmz_dev",
    "env:prod": "node tools/env/generate-env.js prod",
    "env:test": "node tools/env/generate-env.js test",
    "pre-start:dev": "npm run env:dev && npm run quality:quick",
    "pre-start:prod": "npm run env:prod && npm run quality:quick",
    "pre-start:test": "npm run env:test && npm run quality:quick",
    "start": "npm run env:generate dev && ng serve --configuration=dev",
    "start:dev": "npm run pre-start:dev && ng serve --host 0.0.0.0 --port 4411",
    "start:prod": "npm run pre-start:prod && ng serve --host 0.0.0.0 --port 4404",
    "start:test": "npm run pre-start:test && ng serve --host 0.0.0.0 --port 4403",
    "build": "npm run build:dev",
    "build:dev": "npm run env:dev && ng build",
    "build:cloud": "npm run env:cloud && ng build",
    "build:cmz_dev": "npm run env:cmz_dev && ng build",
    "build:prod": "npm run env:prod && ng build --configuration=production",
    "build:test": "npm run env:test && ng build --configuration=test",
    "deploy:test": "npm run build:test && rsync -avh dist/ user@server:/var/www/test-app/",
    "deploy:prod": "npm run build:prod && rsync -avh dist/ user@server:/var/www/prod-app/",
    "quality:quick": "npm run format && npm run lint:fix",
    "quality:full": "npm run quality:quick && npm run lint:all && npm run test:ci",
    "quality:ci": "npm run lint:all && npm run test:ci && npm run security:audit",
    "lint:component": "npx eslint src/app/app.component.ts --debug",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:styles": "stylelint \"src/**/*.css\" \"src/**/*.scss\"",
    "lint:all": "npm run lint && npm run lint:styles",
    "format": "prettier \"src/**/*.{ts,html,css,scss}\" --write",
    "format:check": "prettier \"src/**/*.{ts,html,css,scss}\" --check",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --watch=false --code-coverage",
    "test:all": "npm run test:ci && npm run test:coverage",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:check-dependencies-deprecated": "pnpm why -g",
    "deps:update-dependencies": "npx npm-check-updates",
    "deps:update-dependencies-major": "npx npm-check-updates -u",
    "clean:pnpm-cache": "pnpm store prune",
    "clean:node-modules": "rm -rf dist node_modules/.cache .angular",
    "clean:all-cache": "pnpm clean:all",
    "clean:install": "pnpm run clean:all && pnpm install",
    "clean:rebuild": "npm run clean:install && npm run build:all",
    "build:ci": "npm run quality:ci && npm run build:prod",
    "ci:full": "npm run clean:all && npm run build:ci && npm run security:audit",
    "generate-structure": "powershell -ExecutionPolicy Bypass -File ./scripts/generate-structure.ps1",
    "gsc": "npm run generate-structure",
    "scripts:help": "echo 'ðŸš€ Disponible: start, build, quality, test, deploy, clean'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run quality:quick",
      "pre-push": "npm run quality:full"
    }
  },
  "private": true,
