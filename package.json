        "start:dev": "npm run set-env -- dev && ng serve --host 0.0.0.0 --port 4401",
        "start:prod": "npm run set-env -- prod && ng serve --host 0.0.0.0",
        "build:dev": "npm run set-env -- dev && ng build",
        "build:prod": "npm run set-env -- prod && ng build --configuration=production",
