{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cmz-backoffice": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app", 
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "allowedCommonJsDependencies": [
              "lodash",
              "rxjs-compat",
              "@ks89/angular-modal-gallery",
              "@ckeditor/ckeditor5-build-classic",
              "clone-deep",
              "dropzone",
              "leaflet",
              "feather-icons",
              "mousetrap",
              "hammerjs",
              "firebase/app",
              "dragula",
              "knob",
              "chartist",
              "chartjs-plugin-streaming",
              "@firebase/app",
              "chart.js",
              "dom-set",
              "crossvent",
              "contra/emitter",
              "ev-emitter/ev-emitter",
              "get-size/get-size",
              "outlayer/outlayer",
              "rxjs",
              "ngx-masonry",
              "@firebase/component",
              "@firebase/firestore",
              "@firebase/auth",
              "@firebase/util",
              "rxjs/operators",
              "rxjs/Subject",
              "rxjs/internal/operators",
              "rxjs/internal/observable/of",
              "rxjs/add/operator/map",
              "rxjs/Subscription",
              "masonry-layout",
              "imagesloaded",
              "rxjs-compat/add/operator/map",
              "@mattlewis92/dom-autoscroller",
              "dom-plane",
              "@mattlewis92/dom-autoscroller",
              "@tweenjs/tween.js",
              "photoswipe",
              "photoswipe/dist/photoswipe-ui-default",
              "angular-calendar/date-adapters/date-fns",
              "apexcharts",
              "apexcharts/dist/apexcharts.min.js",
              "calendar-utils/date-adapters/date-fns"
            ],
            "outputPath": "dist/cmz-backoffice",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["src/polyfills.ts"],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets/images/favicon.png",
              {
                "glob": "**/*",
                "input": "src/assets/",
                "ignore": [
                  "**/*.scss"
                ],
                "output": "/assets/"
              },
              "src/assets"
            ],
            "styles": [
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
              "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css",
              "node_modules/leaflet/dist/leaflet.css",
              "./node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
              "./node_modules/leaflet.markercluster/dist/MarkerCluster.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeflex/primeflex.css"
  
            ],
            "scripts": [
              "./node_modules/pusher-js/dist/web/pusher.min.js"
            ],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "test": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test.ts"
                }
              ],
              "optimization": true,
              "sourceMap": true,
              "namedChunks": false
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "9mb",
                  "maximumError": "9mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "9mb",
                  "maximumError": "9mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "cmz-backoffice:build:dev"
          },
          "configurations": {
            "dev": {
              "buildTarget": "cmz-backoffice:build:dev",
              "port": 4411,
              "host": "localhost",
              "ssl": false,
              "liveReload": true,
              "hmr": true
            },
            "test": {
              "buildTarget": "cmz-backoffice:build:test"
            },
            "production": {
              "buildTarget": "cmz-backoffice:build:production"
            }
          },
          "defaultConfiguration": "dev"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "cmz-backoffice:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cmz-backoffice:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "cmz-backoffice:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  }
}